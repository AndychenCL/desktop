msgid "Show previous article"
msgstr "Εμφάνιση προηγούμενου άρθρου"

#: ../ui.py:1260
msgid "N_ext Language"
msgstr "Επόμενη γ_λώσσα"

#: ../ui.py:1261
msgid "Show next language word list"
msgstr "Εμφάνιση αποτελεσμάτων για την επόμενη γλώσσα"

#: ../ui.py:1264
msgid "P_revious Language"
msgstr "Προηγούμενη γλώ_σσα"

